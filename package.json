{
    "name": "form-builder",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "prisma generate && next build",
        "watch-tsc": "tsc --watch --noEmit",
        "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
        "start": "next start",
        "lint": "next lint",
        "prettier": "prettier -w .",
        "postinstall": "prisma generate",
        "lint:write": "eslint --debug src/ --fix --max-warnings=0",
        "prepare": "husky install"
