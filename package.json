        "eslint-config-next": "14.1.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "postcss": "^8",
        "prettier": "3.2.5",
        "prisma": "^5.9.1",
        "tailwindcss": "^3.3.0",
        "typescript": "^5"
